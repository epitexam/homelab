# Global environment variables - Copy this file to .env and fill in your values
# cp .env.example .env

# Timezone (examples: Europe/Paris, America/New_York, UTC)
TZ=Europe/Paris

# IP address of your machine where Docker runs
# Linux/Mac: hostname -I
# Windows: ipconfig
# Use local IP (192.168.x.x or 10.0.x.x)
SERVER_IP=192.168.1.100

# Optional admin port
# Use a port > 1024
ADMIN_PORT=9000
